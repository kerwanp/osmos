{
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "Preserve",
    "moduleResolution": "bundler",
    "noImplicitAny": false,
    "declaration": true,
    "esModuleInterop": true,
    "types": [
      "vite/client",
      "vitest/globals",
      "@osmosjs/router/virtual",
      "@osmosjs/vite/virtual"
    ]
  },
  "exclude": ["dist", "node_modules"],
  "include": ["src"]
}
