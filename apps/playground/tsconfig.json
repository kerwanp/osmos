{
  "extends": "./.osmos/types/tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "allowJs": true,
    "target": "ESNext",
    "module": "Preserve",
    "moduleResolution": "bundler",
    "noImplicitAny": false,
    "declaration": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": ["dist", "node_modules"],
  "include": ["src"]
}
