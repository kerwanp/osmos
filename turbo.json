{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".osmos/**", ".output/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": false
    },
    "prepare": {
      "dependsOn": ["^prepare"],
      "cache": false
    },
    "stub": {
      "dependsOn": ["^stub"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^stub"],
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    }
  }
}
